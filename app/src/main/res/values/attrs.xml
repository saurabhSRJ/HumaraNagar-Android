<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="header" format="string" />
    <attr name="inputEnabled" format="boolean" />
    <attr name="invalidInput" format="boolean" />
    <attr name="hint" format="string" />
    <attr name="input" format="string" />
    <attr name="endIcon" format="integer" />
    <attr name="required_input" format="boolean" />
    <attr name="options" format="reference" />
    <attr name="minimumHeight" format="integer" />

    <declare-styleable name="FormInputWithHeader">
        <attr name="header" />
        <attr name="inputEnabled" />
        <attr name="invalidInput" />
        <attr name="hint" />
        <attr name="input" />
        <attr name="endIcon" />
        <attr name="required_input" />
        <attr name="minimumHeight" />/>

    </declare-styleable>

    <declare-styleable name="FormSpinnerWithHeader">
        <attr name="header" />
        <attr name="hint" />
    </declare-styleable>
</resources>