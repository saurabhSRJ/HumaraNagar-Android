<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/add_user_navigation"
    app:startDestination="@id/mobileNumberVerificationFragment">

    <fragment
        android:id="@+id/mobileNumberVerificationFragment"
        android:name="com.humara.nagar.ui.add_user.MobileNumberVerificationFragment"
        android:label="MobileNumberVerificationFragment">
        <action
            android:id="@+id/action_mobileNumberVerificationFragment_to_webViewFragment2"
            app:destination="@id/webViewFragment" />
        <action
            android:id="@+id/action_mobileNumberVerificationFragment_to_addUserDetailsFragment"
            app:destination="@id/addUserDetailsFragment"
            app:enterAnim="@anim/slide_in_from_right"
            app:exitAnim="@anim/slide_out_to_left"
            app:popEnterAnim="@anim/slide_in_from_left"
            app:popExitAnim="@anim/slide_out_to_right"
            app:popUpTo="@id/mobileNumberVerificationFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/webViewFragment"
        android:name="com.humara.nagar.ui.common.WebViewFragment"
        android:label="WebViewFragment">
        <argument
            android:name="source"
            android:defaultValue=""
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue=""
            app:argType="string" />
        <argument
            android:name="url"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/addUserDetailsFragment"
        android:name="com.humara.nagar.ui.add_user.AddUserDetailsFragment"
        android:label="AddUserDetailsFragment" />
</navigation>